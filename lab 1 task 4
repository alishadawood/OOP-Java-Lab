
package area;

public class Area {
Double length;
Double width;
Double Area;
  
    public static void main(String[] args) {
        Area A=new Area();
       Double L;
       L=A.length=2.0;
       Double W;
       W=A.width=3.0;
       Double Area=L*W;
       System.out.println(Area);
       
    }
    
}
