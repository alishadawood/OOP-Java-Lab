
package lab1;
public class Lab1 {
String name;
String city;
int age;
public void getdetail(){
    System.out.println("name is= "+name+ "\ncity is= "+city+"\n age = "+age);
    
}
    public static void main(String[] args) {
       Lab1 s1=new Lab1();
      Lab1 s2=new Lab1();
      s1.name="alisha";
      s1.city="islambad";
      s1.age=19;
      s2.name="arfa";
      s2.city="karachi";
      s2.age=20;
     s1.getdetail();
     s2.getdetail();
    }
    
}
